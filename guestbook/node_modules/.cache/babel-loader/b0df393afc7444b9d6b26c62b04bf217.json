 this.$refs.msgListToolbar.computedContentHeight;\r\n  },\r\n  created() {\r\n    let url = \"sda\";\r\n\r\n    const regex = /[0-9]{3,15}([a-z]){3,15}|([a-z]){3,15}[0-9]{3,15}|[a-z]{3,15}/gi;\r\n    const matches = regex.exec(url);\r\n    console.info(matches);\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/components"}]}